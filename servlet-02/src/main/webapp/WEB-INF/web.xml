<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                                http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">

    <context-param>
        <param-name>url</param-name>
        <param-value>jdbc:mysql://localhost::3306/mybatis</param-value>
<!--        <param-value>jdbc:postgresql://192.168.0.20::18054/FeiyuLiu</param-value>-->
    </context-param>

    <!--hello02-->
    <servlet>
        <servlet-name>hello02</servlet-name>
        <servlet-class>com.neusoft.servlet.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello02</servlet-name>
        <url-pattern>/hello02</url-pattern>
    </servlet-mapping>

    <!--getContext-->
    <servlet>
        <servlet-name>getc</servlet-name>
        <servlet-class>com.neusoft.servlet.GetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getc</servlet-name>
        <url-pattern>/getc</url-pattern>
    </servlet-mapping>

    <!--getParameter-->
    <servlet>
        <servlet-name>gp</servlet-name>
        <servlet-class>com.neusoft.servlet.Servlet03</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gp</servlet-name>
        <url-pattern>/gp</url-pattern>
    </servlet-mapping>

    <!--s4 -->
    <servlet>
        <servlet-name>s4</servlet-name>
        <servlet-class>com.neusoft.servlet.Servlet04</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/s4</url-pattern>
    </servlet-mapping>

    <!--s5 -->
    <servlet>
        <servlet-name>s5</servlet-name>
        <servlet-class>com.neusoft.servlet.Servlet05</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>s5</servlet-name>
        <url-pattern>/s5</url-pattern>
    </servlet-mapping>

</web-app>